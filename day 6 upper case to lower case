#include <stdio.h>
#include <ctype.h> 
#include <string.h> 

int main() {
    char str[] = "HELLO WORLD 123!:
    int i;

    printf("Original string: %s\n", str);
    for (i = 0; str[i] != '\0'; i++) {
        str[i] = tolower(str[i]);
    }
    printf("Lowercase string: %s\n", str);

    return 0;
}
