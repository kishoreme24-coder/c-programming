#include <stdio.h>


int countOccurrences(int arr[], int size, int targetElement) {
    int count = 0; 


    for (int i = 0; i < size; i++) {
        
        if (arr[i] == targetElement) {
            count++;
        }
    }
    return count; 
}

int main() {
    int myArray[] = {1, 2, 3, 4, 2, 5, 2, 6};
    int arraySize = sizeof(myArray) / sizeof(myArray[0]); 
    int elementToFind = 2;

    
    int occurrences = countOccurrences(myArray, arraySize, elementToFind);


    printf("The element %d occurs %d times in the array.\n", elementToFind, occurrences);

    return 0;
}
