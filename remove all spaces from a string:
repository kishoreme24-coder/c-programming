include <stdio.h>
#include <string.h> 

int main() {
    char str[100], noSpace[100];
    int i, j = 0;

    printf("Enter a string: ");
    fgets(str, sizeof(str), stdin);

    str[strcspn(str, "\n")] = '\0';

    for (i = 0; str[i] != '\0'; i++) {
        if (str[i] != ' ') {
            noSpace[j++] = str[i];
        }
    }
    noSpace[j] = '\0';  // Null-terminate the result

    printf("String without spaces: %s\n", noSpace);

    return 0;
}
